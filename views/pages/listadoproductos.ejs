<!DOCTYPE html>
<html>
    <head>
        <%- include('../partials/head.ejs'); %>
    </head>
    <body>
        <header>
            <%- include('../partials/header.ejs'); %>
        </header>
        <main>
            <table class="table table-striped">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Titulo</th>
                    <th scope="col">Precio</th>
                    <th scope="col">Link Imagen</th>
                  </tr>
                </thead>
                <tbody>
                    <% item.forEach(i => { %>
                        <tr><th scope="row"> - </th><td><%= i.title %></td><td><%= i.price %></td><td><%= i.thumbnail %></td></tr>
                     <% }); %> 

                    
                  
                </tbody>
              </table>
        </main>
        <footer>
            <%- include('../partials/footer.ejs'); %>
        </footer>
    </body>
</html>